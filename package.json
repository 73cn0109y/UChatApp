{
  "name": "uchat",
  "version": "1.0.0",
  "description": "Multi-platform chat program for Windows, Mac & Linux",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-dev": "set NODE_ENV=development&& webpack --watch",
    "build": "webpack",
    "dist-win32": "npm run build && electron-packager . uchat --platform=win32 --arch=ia32 --out=dist/ --overwrite",
    "dist-win64": "npm run build && electron-packager . uchat --platform=win32 --arch=x64 --out=dist/ --overwrite",
    "dist-linux32": "npm run build && electron-packager . uchat --platform=linux --arch=ia32 --out=dist/ --overwrite",
    "dist-linux64": "npm run build && electron-packager . uchat --platform=linux --arch=x64 --out=dist/ --overwrite"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "UChat",
    "Chat",
    "cross-platform",
    "multi-platform",
    "twitch",
    "twitch.tv",
    "liveedu",
    "liveedu.tv",
    "livecoding",
    "livecoding.tv",
    "beam",
    "beam.pro",
    "youtube"
  ],
  "author": "David Sattler <dsmine1737@gmail.com>",
  "license": "CC0-1.0",
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/compiler-cli": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/forms": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/platform-server": "^2.4.2",
    "@angular/router": "^3.4.2",
    "@types/node": "^7.0.0",
    "bootstrap": "^4.0.0-alpha.6",
    "electron-config": "^0.2.1",
    "electron-json-storage": "^3.0.0",
    "jquery": "^3.1.1",
    "rxjs": "^5.0.3",
    "socket.io-client": "^1.7.2",
    "tether": "^1.4.0",
    "twitch-emoji": "^0.2.43",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.1",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "core-js": "^2.4.1",
    "electron": "^1.4.1",
    "electron-builder": "^11.3.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.2.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "ts-loader": "^1.3.3",
    "ts-node": "^2.0.0",
    "typescript": "^2.0.2",
    "webpack": "^1.14.0"
  }
}
